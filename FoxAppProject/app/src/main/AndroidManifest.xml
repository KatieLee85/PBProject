<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foxappproject">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoxAppProject">
        <activity android:name=".RandomPics"></activity>
        <activity android:name=".whichFoxAreYou"></activity>
        <activity android:name=".quizFoxAnswerA"></activity>
        <activity android:name=".quizFoxAnswerB"></activity>
        <activity android:name=".quizFoxAnswerC"></activity>
        <activity android:name=".quizFoxAnswerD"></activity>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>